{
  "name": "@vue-storefront/vendure-api",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "server": "server/index.js",
  "main": "lib/index.cjs.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rimraf lib server && rollup -c",
